# - Config file for the apriltagscpp package
# It defines the following variables
#  apriltagscpp_INCLUDE_DIRS - include directories for FooBar
#  apriltagscpp_LIBRARIES    - libraries to link against
#  apriltagscpp_EXECUTABLE   - the bar executable
 
# Compute paths
get_filename_component(apriltagscpp_CMAKE_DIR "${CMAKE_CURRENT_LIST_FILE}" PATH)
set(apriltagscpp_INCLUDE_DIRS "@CONF_INCLUDE_DIRS@")

# Our library dependencies (contains definitions for IMPORTED targets)
if(NOT TARGET apriltagscpp AND NOT apriltagscpp_BINARY_DIR)
  include("${apriltagscpp_CMAKE_DIR}/apriltagscppTargets.cmake")
endif()

# These are IMPORTED targets created by FooBarTargets.cmake
set(apriltagscpp_LIBRARIES apriltagscpp)
set(apriltagscpp_EXECUTABLE apriltagscpp camtest tagtest quadtest gltest maketags)

